{
  "name": "juka_website",
  "version": "1.0.0",
  "description": "Juka Programming Language website!",
  "main": "index.js",
  "scripts": {
    "build": "cleandir ./.cache && gatsby build",
    "develop": "cleandir ./.cache && gatsby develop",
    "start": "npm run develop",
    "lint": "eslint ./src",
    "setup": "node ./bin/setup.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/website-jukalang.com"
  },
  "keywords": [
    "Juka",
    "Juka Programming Language",
    "JPL",
    "Open Source",
    "Programming"
  ],
  "author": "Juka Language Team",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/jukaLang/website-jukalang.com"
  },
  "homepage": "https://jukalang.com",
  "dependencies": {
    "clean-dir": ">=1.0.3",
    "contentful": "^7.5.0",
    "dotenv": "^7.0.0",
    "envfile": ">=3.0.0",
    "gatsby": "^2.3.22",
    "gatsby-plugin-google-analytics": "^2.0.18",
    "gatsby-plugin-google-fonts": ">=0.0.4",
    "gatsby-plugin-manifest": "^2.0.29",
    "gatsby-plugin-offline": "^2.0.25",
    "gatsby-plugin-react-helmet": "^3.0.12",
    "gatsby-plugin-styled-components": "^3.0.7",
    "gatsby-source-contentful": "^2.0.48",
    "gatsby-transformer-remark": "^2.3.8",
    "inquirer": ">=6.2.0",
    "prettier": "^1.17.0",
    "prop-types": ">=15.6.2",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-fontawesome": ">=1.6.1",
    "react-headroom": "^2.2.8",
    "react-helmet": ">=5.2.0",
    "react-markdown": ">=4.0.3",
    "react-reveal": ">=1.2.2",
    "react-scroll-section": ">=1.3.0",
    "react-text-loop": ">=2.0.1",
    "react-tippy": ">=1.2.2",
    "react-twitter-embed": "^2.0.7",
    "rebass": "^3.1.0",
    "styled-components": "^4.2.0"
  },
  "devDependencies": {
    "chalk": ">=2.4.1",
    "contentful-import": ">=7.4.0",
    "eslint": "^5.16.0",
    "eslint-config-airbnb": ">=17.1.0",
    "eslint-config-prettier": "^4.1.0",
    "eslint-plugin-import": "^2.17.0",
    "eslint-plugin-jsx-a11y": ">=6.1.1",
    "eslint-plugin-react": ">=7.12.3",
    "husky": ">=1.3.1",
    "lint-staged": ">=8.1.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,json,css,md}": [
      "prettier --write",
      "git add"
    ]
  }
}
